{
   "action": {
      "default_popup": "index.html",
      "default_title": "Bing Search Automator"
   },
   "author": "A Mahapatra",
   "background": {
      "service_worker": "./js/background.js"
   },
   "description": "Chrome / Edge extension for Automatic Bing Search",
   "icons": {
      "128": "./img/icon128.png",
      "16": "./img/icon16.png",
      "48": "./img/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Bing Search Automator",
   "permissions": [ "debugger","tabs","storage", "declarativeNetRequest", "alarms" ,"https://*.bing.com/*", "https://account.microsoft.com/rewards/*", "https://trends.google.com/*", "webRequest", "webRequestBlocking" ],
   "version": "1.3.1"
}
